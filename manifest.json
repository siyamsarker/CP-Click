{
    "manifest_version":  3,
    "name":  "CP Click",
    "description":  "ChalkPencil.com Quick Launch",
    "version":  "1.0.0",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "action": {
        "default_title": "CP Click",
        "default_popup": "popup.html"
      },
      "permissions": [
        "activeTab",
        "scripting"
      ],
      "background": {
        "service_worker": "background.js"
      }
}